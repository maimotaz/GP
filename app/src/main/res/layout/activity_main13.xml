<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main13"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/background"
    tools:context="com.example.mai.gp.Main13Activity">

    <TextView

        android:text=""
        android:background="@drawable/arow"
        android:layout_width="40sp"
        android:layout_height="35sp"
        android:id="@+id/textView30"
        android:layout_weight="0.75" />

    <TextView
        android:text=""
        android:layout_width="200sp"
        android:layout_height="50sp"
        android:background="@drawable/zwara_word"
        android:id="@+id/textView34"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView

        android:text=""
        android:layout_width="40sp"
        android:background="@drawable/person"
        android:layout_height="35sp"
        android:id="@+id/textView29"
        android:layout_weight="1"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="16dp" />

    <TextView
        android:text="قائمة المستخدمين"
        android:textColor="#D98518"
        android:textSize="30sp"
        android:layout_width="wrap_content"
        android:layout_height="50sp"
        android:id="@+id/textView41"
        android:layout_marginTop="45dp"
        android:layout_below="@+id/searchView"
        android:layout_alignEnd="@+id/textView29" />

    <TextView
        android:text="بحث"
        android:textColor="#D98518"
        android:textSize="30sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="41dp"
        android:id="@+id/textView42"
        android:layout_below="@+id/textView34"
        android:layout_alignParentEnd="true"
        android:layout_alignStart="@+id/textView29" />

    <SearchView
        android:layout_width="match_parent"
        android:background="#FFEEE3D6"
        android:layout_height="50sp"
        android:id="@+id/searchView"
        android:layout_alignTop="@+id/textView42"
        android:layout_alignParentStart="true"
        android:layout_toStartOf="@+id/textView42">

    </SearchView>

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="50sp"
        android:background="@drawable/picture3"
        android:inputType="textPersonName"
        android:textSize="20sp"
        android:textColor="#8C5812"
        android:text="     بياتو"
        android:ems="10"
        android:id="@+id/editText11"
        android:layout_centerVertical="true"
        android:layout_alignParentStart="true" />

    <Button
        android:text=""
        android:background="@drawable/add_restaurant"
        android:layout_width="wrap_content"
        android:layout_height="60sp"
        android:id="@+id/button15"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="27dp"
        android:layout_alignEnd="@+id/textView29" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/piato"
        android:id="@+id/imageView9"
        android:layout_centerVertical="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="50sp"
        android:background="@drawable/picture3"
        android:inputType="textPersonName"
        android:textSize="20sp"
        android:textColor="#8C5812"
        android:text="      شيليز"
        android:ems="10"
        android:id="@+id/editText10"
        android:layout_marginTop="11dp"
        android:layout_alignTop="@+id/imageView10"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/shilise"
        android:id="@+id/imageView10"
        android:layout_below="@+id/imageView9"
        android:layout_alignEnd="@+id/imageView9" />

</RelativeLayout>
