<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main12"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/background"
    tools:context="com.example.mai.gp.Main12Activity">


    <TextView

        android:text=""
        android:background="@drawable/arow"
        android:layout_width="40sp"
        android:layout_height="35sp"
        android:id="@+id/textView30"
        android:layout_weight="0.75" />

    <TextView
        android:text=""
        android:layout_width="200sp"
        android:layout_height="50sp"
        android:background="@drawable/zwara_word"
        android:id="@+id/textView34"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView

        android:text=""
        android:layout_width="40sp"
        android:background="@drawable/person"
        android:layout_height="35sp"
        android:id="@+id/textView29"
        android:layout_weight="1"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="16dp" />

    <Button
        android:text=""
        android:background="@drawable/regester"
        android:layout_width="wrap_content"
        android:layout_height="60sp"
        android:id="@+id/button15"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="27dp" />


    <EditText
        android:layout_width="wrap_content"
        android:layout_height="60sp"
        android:textColor="#A49279"
        android:layout_marginTop="13dp"
        android:textAlignment="center"
        android:inputType="textPassword"
        android:text="كلمة المرور"

        android:ems="10"
        android:id="@+id/editText8"
        android:layout_below="@+id/editText5"

        android:layout_alignEnd="@+id/editText5"
        android:layout_toEndOf="@+id/textView30" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="60sp"
        android:inputType="textPersonName"
        android:text="معلومات عن المستخدم"
        android:ems="10"
        android:id="@+id/editText7"
        android:layout_below="@+id/editText8"
        android:layout_marginTop="15dp"
        android:layout_alignEnd="@+id/editText8"
        android:textAlignment="center"
        android:textColor="#A49279"
        android:layout_toEndOf="@+id/textView30" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="60sp"
        android:inputType="textPersonName"
        android:text="اسم المستخدم"
        android:textColor="#A49279"
        android:ems="10"
        android:id="@+id/editText5"
        android:textAlignment="center"
        android:layout_below="@+id/textView34"
        android:layout_alignEnd="@+id/textView29"
        android:layout_toEndOf="@+id/textView30" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/kshk"
        android:id="@+id/imageView5"
        android:layout_alignEnd="@+id/textView29"
        android:layout_below="@+id/textView34"
        android:layout_toEndOf="@+id/textView34"
        android:layout_above="@+id/editText8" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/password"
        android:layout_above="@+id/editText7"
        android:layout_marginBottom="12dp"
        android:id="@+id/imageView6"
        android:layout_alignEnd="@+id/editText8"
        android:layout_below="@+id/editText5"
        android:layout_toEndOf="@+id/textView34"
        android:layout_alignStart="@+id/imageView5" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/user_info"
        android:id="@+id/imageView7"
        android:layout_alignStart="@+id/textView29"
        android:layout_alignEnd="@+id/editText7"
        android:layout_below="@+id/editText8" />

    <TextView
        android:text="الموقع"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/editText7"
        android:layout_alignEnd="@+id/editText7"
        android:id="@+id/textView39"
        android:textStyle="normal|bold"
        android:textSize="20sp"
        android:textColor="@color/colorPrimaryDark" />

    <EditText
        android:layout_width="100sp"

        android:layout_height="60sp"
        android:inputType="textPersonName"
        android:text="رقم الهاتف"
        android:ems="10"
        android:id="@+id/editText6"
        android:textAlignment="center"
        android:textColor="#A49279"
        android:layout_above="@+id/button15"
        android:layout_alignEnd="@+id/textView39"
        android:layout_marginBottom="26dp"
        android:layout_toEndOf="@+id/textView30" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/phone"
        android:id="@+id/imageView8"
        android:layout_alignTop="@+id/editText6"
        android:layout_alignEnd="@+id/editText6"
        android:layout_alignBottom="@+id/editText6"
        android:layout_alignStart="@+id/imageView6" />

</RelativeLayout>
